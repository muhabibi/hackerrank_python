import math
import os
import random
import re
import sys

#
# Complete the 'miniMaxSum' function below.
#
# The function accepts INTEGER_ARRAY arr as parameter.
#

def miniMaxSum(arr):
    # Write your code here
    minsort = sorted(arr)
    maxsort = sorted(arr, reverse=True)
    
    minv, maxv = (0,0)
    
    for i in range(4):
        minv += minsort[i]
        maxv += maxsort[i]
    print(minv, maxv)
if __name__ == '__main__':

    arr = list(map(int, input().rstrip().split()))

    miniMaxSum(arr)
